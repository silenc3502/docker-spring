<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="controller"
		class="com.example.singlebean.controller.FixedDepositControllerImpl">
		<property name="fixedDepositService" ref="service" />
	</bean>

	<bean id="service"
		class="com.example.singlebean.service.FixedDepositServiceImpl">
		<property name="fixedDepositDao" ref="dao"/>
	</bean>

	<bean id="dao"
		class="com.example.singlebean.dao.FixedDepositDaoImpl" />

	<bean id="fixedDepositDetails"
		class="com.example.singlebean.domain.FixedDepositDetails"
		scope="prototype" />

	<bean id="anotherDao"
		class="com.example.singlebean.dao.FixedDepositDaoImpl" />

</beans>
